{
  "name": "dydx-market-making-bot",
  "version": "0.3.0",
  "description": "Market making bot for dydx exchange",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start-prod": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/start.js",
    "start-docker": "ts-node -r tsconfig-paths/register ./src/start.ts",
    "start": "nodemon --config \"./util/nodemon.json\"",
    "test": "nodemon --config \"./util/nodemon.test.json\""
  },
  "_moduleAliases": {
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@dydxprotocol/solo": "^0.15.1",
    "@types/cors": "^2.8.6",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.16",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "module-alias": "^2.2.1",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "web3": "^1.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.4.0",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.2",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.4.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/zaregoclients/ctf-dydx-market-making-bot.git"
  },
  "keywords": [
    "dydx",
    "blockchain",
    "ethereum"
  ],
